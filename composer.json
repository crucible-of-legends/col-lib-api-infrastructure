{
  "name": "crucible-of-legends/col-lib-api-infrastructure",
  "description": "A library to share api infrastructure",
  "homepage": "https://github.com/crucible-of-legends/col-lib-api-infrastructure",
  "type": "library",
  "license": "proprietary",
  "keywords": [],
  "autoload": {
    "psr-4": {
      "COL\\Library\\Infrastructure\\": "src/"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "crucible-of-legends/col-lib-base-contracts": "1.x-dev",
    "crucible-of-legends/col-lib-api-contracts": "1.x-dev",
    "symfony/http-foundation": "^5.1",
    "doctrine/persistence": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "friendsofphp/php-cs-fixer": "~3.1.0",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "phpro/grumphp": "^1.3.1",
    "phpstan/phpstan": "^0.12.25",
    "roave/security-advisories": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/crucible-of-legends/col-lib-base-contracts"
    },
    {
      "type": "vcs",
      "url": "https://github.com/crucible-of-legends/col-lib-api-contracts"
    }
  ],
  "config": {
    "platform": {
    }
  },
  "authors": [
    {
      "name": "Ghriim",
      "email": "crucible.of.legends@gmail.com"
    }
  ]
}
